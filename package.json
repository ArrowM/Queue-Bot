{
   "name": "queue-bot",
   "version": "1.0.0",
   "description": "A Discord bot!",
   "main": "build/bot.js",
   "engines": {
      "node": "15.14.x"
   },
   "scripts": {
      "build": "tsc -p .",
      "format": "eslint src/** --fix --no-error-on-unmatched-pattern",
      "start": "cd build && node bot.js",
      "restart": "tsc -p . && pm2 stop all && pm2 flush && pm2 start all",
      "dev": "ts-node-dev -r tsconfig-paths/register --inspect --respawn --transpile-only --ignore-watch node_modules --no-notify src/bot.ts"
   },
   "keywords": [],
   "author": "Arrow",
   "license": "ISC",
   "type": "commonjs",
   "dependencies": {
      "bufferutil": "^4.0.3",
      "dblapi.js": "^2.4.1",
      "discord.js": "12.5.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "knex": "^0.95.4",
      "libsodium-wrappers": "^0.7.9",
      "node-gyp": "^8.0.0",
      "pg": "^8.5.1",
      "utf-8-validate": "^5.0.4",
      "zlib-sync": "^0.1.7"
   },
   "devDependencies": {
      "@types/express": "^4.17.11",
      "@types/node": "^14.14.37",
      "@typescript-eslint/parser": "^4.20.0",
      "eslint": "^7.23.0",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "eslint-plugin-promise": "^4.3.1",
      "prettier": "^2.2.1",
      "ts-node-dev": "^1.1.6",
      "typescript": "^4.2.3"
   }
}
