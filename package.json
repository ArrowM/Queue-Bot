{
  "name": "queue-bot",
  "version": "1.0.0",
  "description": "A Discord bot!",
  "main": "build/bot.js",
  "scripts": {
    "build": "tsc -p .",
    "format": "eslint src/**/*.ts --fix --no-error-on-unmatched-pattern && prettier --write --loglevel warn \"src/**/*.ts\"",
    "start": "cd build && node bot.js",
    "restart": "tsc -p . && pm2 stop all && pm2 flush && pm2 start all"
  },
  "keywords": [],
  "author": "Arrow",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@discordjs/voice": "^0.8.0",
    "@types/node-cron": "^3.0.1",
    "bufferutil": "^4.0.3",
    "cronstrue": "^1.125.0",
    "delay": "^5.0.0",
    "discord-slash-commands-client": "^1.2.1",
    "discord.js": "^13.2.0",
    "knex": "^1.0.1",
    "knex-schema-inspector": "^1.5.8",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "node-gyp": "^8.1.0",
    "pg": "^8.6.0",
    "string-similarity": "^4.0.4",
    "topgg-autoposter": "^2.0.0",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.1",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 100,
    "tabWidth": 2
  }
}
