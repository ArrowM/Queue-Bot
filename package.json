{
  "name": "queue-bot",
  "version": "1.0.0",
  "description": "A Discord bot!",
  "main": "build/bot.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts --fix && prettier --write --loglevel warn src/**/*.ts",
    "start": "cd build && node bot.js",
    "restart": "tsc -p . && pm2 restart bot",
    "restart:flush": "tsc -p . && pm2 stop bot && pm2 flush bot && pm2 start bot"
  },
  "keywords": [],
  "author": "Arrow",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@discordjs/voice": "^0.11.0",
    "cronstrue": "^2.11.0",
    "delay": "^5.0.0",
    "discord-slash-commands-client": "^1.2.2",
    "discord.js": "^13.9.2",
    "knex": "^2.2.0",
    "knex-schema-inspector": "^2.0.3",
    "lodash": "^4.17.21",
    "multikey-map": "^0.2.1",
    "node-cron": "^3.0.1",
    "node-gyp": "^9.1.0",
    "pg": "^8.7.3",
    "sodium-native": "^3.3.0",
    "topgg-autoposter": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/node-cron": "^3.0.2",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 140,
    "tabWidth": 2
  }
}
